#include<stdio.h>
#include<math.h>
 
int main()
{
    int a[24][24],i,j,p,q,temp,sum=0;
 
 
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++){
 
        scanf("%d",&a[i][j]);
        if(a[i][j]==1)
        {
            p=i;
            q=j;
        }
    }
 }
 
    sum=abs(p-2)+abs(q-2);
 printf("%d\n",sum);
}
